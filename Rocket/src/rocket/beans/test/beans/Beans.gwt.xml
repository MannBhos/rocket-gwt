<module>
	<inherits name="com.google.gwt.junit.JUnit"/>
	<inherits name="rocket.beans.Beans"/>
	<inherits name="rocket.generator.Generator"/>
	<inherits name="rocket.remoting.Remoting"/>
	
	<source path="client" />

	<generate-with class="rocket.beans.test.beans.rebind.TestBeanFactoryGenerator">
		<when-type-assignable class="rocket.beans.test.beans.client.notabeanfactory.ClassIsNotABeanFactory" />
	</generate-with>	
	
	<generate-with class="rocket.beans.test.beans.rebind.TestBeanFactoryGenerator">
		<when-type-assignable class="rocket.beans.client.BeanFactory" />
	</generate-with>	

	<servlet path="/advised-rpc" class="rocket.beans.test.beans.server.advisedrpc.AdvisedGwtRpcImpl"/>	
</module>

