<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE bean-factory PUBLIC "-//rocket-gwt//Beans 0.40//EN" "http://code.google.com/p/rocket-gwt">
<bean-factory>
	<bean 	id="bean"
			class="rocket.beans.test.beans.client.doublenestedbean.BeanWithDoubleNestedBean" 
			scope="singleton">		
		<properties>
			<property name="nestedBeanWithNestedBean">
				<!-- nested bean -->
				<bean	class="rocket.beans.test.beans.client.doublenestedbean.NestedBeanWithNestedBean" 
						scope="singleton">			
					<properties>
						<property name="nestedBean">			
							<!-- nested nested bean -->
							<bean	class="rocket.beans.test.beans.client.doublenestedbean.NestedBean" 
									scope="singleton" />												
						</property>
					</properties>						
				</bean>
			</property>
		</properties>
	</bean>

</bean-factory>