<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE bean-factory PUBLIC "-//rocket-gwt//Beans 1.0//EN" "http://code.google.com/p/rocket-gwt">
<bean-factory>
	<bean 
		id="factory" 
		class="rocket.beans.test.client.factorymethod.FactoryMethodBean" 
		scope="singleton"
		>
	</bean>

	<bean 
		id="bean" 
		class="rocket.beans.test.client.factorymethod.Bean" 
		scope="singleton"
		>
		<factory
			reference-id="factory"
			method="create"
			/>		
	</bean>
</bean-factory>