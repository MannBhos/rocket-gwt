<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE bean-factory PUBLIC "-//rocket-gwt//Beans 1.0//EN" "http://code.google.com/p/rocket-gwt">
<bean-factory>
	<bean 
		id="bean" 
		class="rocket.beans.test.client.setproperty.ClassWithSetProperty" 
		scope="singleton">
		<property name="setProperty">
			<set>
				<value>apple</value>
				<value>banana</value>
				<value>carrot</value>								
			</set>
		</property>
	</bean>
</bean-factory>