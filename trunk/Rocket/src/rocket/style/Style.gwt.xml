<module>
	<replace-with class="rocket.style.client.support.InternetExplorer6StyleHelperSupport">
		<when-type-is class="rocket.style.client.support.StyleHelperSupport"/>
		<when-property-is name="user.agent" value="ie6"/>
	</replace-with>
	
	<replace-with class="rocket.style.client.support.FireFoxStyleHelperSupport">
		<when-type-is class="rocket.style.client.support.StyleHelperSupport"/>
		<when-property-is name="user.agent" value="gecko"/>
	</replace-with>
	
	<replace-with class="rocket.style.client.support.FireFoxStyleHelperSupport">
		<when-type-is class="rocket.style.client.support.StyleHelperSupport"/>
		<when-property-is name="user.agent" value="gecko1_8"/>
	</replace-with>
	
	<replace-with class="rocket.style.client.support.OperaStyleHelperSupport">
		<when-type-is class="rocket.style.client.support.StyleHelperSupport"/>
		<when-property-is name="user.agent" value="opera"/>
	</replace-with>

</module>