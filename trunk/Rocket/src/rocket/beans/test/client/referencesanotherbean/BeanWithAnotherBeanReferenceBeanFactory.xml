<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE bean-factory PUBLIC "-//rocket-gwt//Beans 1.0//EN" "http://code.google.com/p/rocket-gwt">
<bean-factory>
	<bean 
		id="bean" 
		class="rocket.beans.test.client.referencesanotherbean.ClassWithBeanReference" 
		scope="singleton">
		<property name="anotherBean">
			<bean-reference reference-id="anotherBean" />
		</property>
	</bean>

	<bean 
		id="anotherBean" 
		class="rocket.beans.test.client.referencesanotherbean.AnotherBean" 
		scope="singleton">
	</bean>
</bean-factory>