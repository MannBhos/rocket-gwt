<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE bean-factory PUBLIC "-//rocket-gwt//Beans 1.0//EN" "http://code.google.com/p/rocket-gwt">
<bean-factory>
	<bean 
		id="bean" 
		class="rocket.beans.test.beans.client.rpcproperty.BeanWithGwtRpcService" 
		scope="singleton">
		<properties>
			<property name="service">
				<bean-reference 
					reference-id="remoteGwtRpcService" />
			</property>
		</properties>
	</bean>

	<remote-rpc-service
		id="remoteGwtRpcService"
		interface="rocket.beans.test.beans.client.rpcproperty.GwtRpcService"
		address="/remoteRpcService"	
	/>
</bean-factory>