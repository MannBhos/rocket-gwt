<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE bean-factory PUBLIC "-//rocket-gwt//Beans 0.40//EN" "http://code.google.com/p/rocket-gwt">
<bean-factory>
	<bean	id="bean" 
			class="rocket.beans.test.beans.client.interceptedlongmethod.ClassWithLongMethod" 
			scope="singleton" />

	<bean 	id="advisor" 
			class="rocket.beans.test.beans.client.interceptedlongmethod.LongMethodInterceptor" 
			scope="singleton" />
	
	<aspect	advisor="advisor"
			target="bean"
			methods="add"	/>
</bean-factory>