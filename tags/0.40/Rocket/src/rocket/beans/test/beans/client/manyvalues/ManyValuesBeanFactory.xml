<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE bean-factory PUBLIC "-//rocket-gwt//Beans 0.40//EN" "http://code.google.com/p/rocket-gwt">
<bean-factory>
	<bean 
		id="bean" 
		class="rocket.beans.test.beans.client.manyvalues.ClassWithManyValues" 
		scope="singleton">
		<properties>
			<property name="apple">
				<value>green</value>
			</property>
			<property name="banana">
				<value>yellow</value>
			</property>
			<property name="carrot">
				<value>orange</value>
			</property>
		</properties>
	</bean>
</bean-factory>