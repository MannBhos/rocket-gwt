<module>
	<replace-with class="rocket.remoting.client.impl.InternetExplorer6CometImpl">
		<when-type-is class="rocket.remoting.client.impl.CometImpl"/>
		<when-property-is name="user.agent" value="ie6"/>
	</replace-with>
  	<replace-with class="rocket.remoting.client.impl.SafariCometImpl">
		<when-type-is class="rocket.remoting.client.impl.CometImpl"/>
		<when-property-is name="user.agent" value="webkit"/>
	</replace-with>	
</module>