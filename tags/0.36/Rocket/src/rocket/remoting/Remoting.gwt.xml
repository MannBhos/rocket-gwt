<module>
	<replace-with class="rocket.remoting.client.support.InternetExplorer6CometSupport">
		<when-type-is class="rocket.remoting.client.support.CometSupport"/>
		<when-property-is name="user.agent" value="ie6"/>
	</replace-with>

  	<replace-with class="rocket.remoting.client.support.SafariCometSuport">
		<when-type-is class="rocket.remoting.client.support.CometSupport"/>
		<when-property-is name="user.agent" value="webkit"/>
	</replace-with>	

	<generate-with class="rocket.remoting.rebind.json.RemoteJsonServiceGenerator">
		<when-type-assignable class="rocket.remoting.client.json.RemoteJsonService" />
	</generate-with>
	
	<generate-with class="rocket.remoting.rebind.comet.CometGenerator">
		<when-type-assignable class="rocket.remoting.client.CometClient" />
	</generate-with>	
</module>