<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE bean-factory PUBLIC "-//rocket-gwt//Beans 0.40//EN" "http://code.google.com/p/rocket-gwt">
<bean-factory>
	<bean 
		id="bean" 
		class="rocket.beans.test.generator.properties.rpc.HasGwtRpcService" 
		scope="singleton">
		<properties>
			<property name="rpc">
				<bean-reference 
					reference-id="rpc" />
			</property>
		</properties>
	</bean>

	<rpc	id="rpc"
			service-interface="rocket.beans.test.generator.properties.rpc.GwtRpc"
			service-entry-point="/rpc"	
	/>
</bean-factory>