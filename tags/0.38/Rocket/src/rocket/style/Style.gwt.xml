<module>
	<!--
	*** StyleSheet support classes **********************************************************************
	-->
	<replace-with class="rocket.style.client.support.InternetExplorerStyleSheetSupport">
		<when-type-is class="rocket.style.client.support.StyleSheetSupport"/>
		<when-property-is name="user.agent" value="ie6"/>
	</replace-with>
	
	<replace-with class="rocket.style.client.support.FireFoxStyleSheetSupport">
		<when-type-is class="rocket.style.client.support.StyleSheetSupport"/>
		<when-property-is name="user.agent" value="gecko"/>
	</replace-with>
	
	<replace-with class="rocket.style.client.support.FireFoxStyleSheetSupport">
		<when-type-is class="rocket.style.client.support.StyleSheetSupport"/>
		<when-property-is name="user.agent" value="gecko1_8"/>
	</replace-with>
	
	<replace-with class="rocket.style.client.support.OperaStyleSheetSupport">
		<when-type-is class="rocket.style.client.support.StyleSheetSupport"/>
		<when-property-is name="user.agent" value="opera"/>
	</replace-with>

	<replace-with class="rocket.style.client.support.SafariStyleSheetSupport">
		<when-type-is class="rocket.style.client.support.StyleSheetSupport"/>
		<when-property-is name="user.agent" value="safari"/>
	</replace-with>
	
	
