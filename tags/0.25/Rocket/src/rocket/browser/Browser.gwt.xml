<module>
	<replace-with class="rocket.browser.client.support.InternetExplorer6BrowserHelperSupport">
		<when-type-is class="rocket.browser.client.support.BrowserHelperSupport"/>
		<when-property-is name="user.agent" value="ie6"/>
	</replace-with>
	
	<replace-with class="rocket.browser.client.support.OperaBrowserHelperSupport">
		<when-type-is class="rocket.browser.client.support.BrowserHelperSupport"/>
		<when-property-is name="user.agent" value="opera"/>
	</replace-with>	
</module>